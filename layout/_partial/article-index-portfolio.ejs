<% if (item.photos && item.photos.length){ %>
	<% if (item.photos.length){ %>
		<% item.photos.forEach(function(photo){ %>
			<% if (item.photos.length==1){ %>
				<div class="work-full">
			<% } else { %>
				<div class="work">
			<% } %>
					<% if (config.lazyload.enable && item.loadingImg){ %>
						<img src="<%- item.loadingImg %>" data-original="<%- photo %>" class="media" />
					<% } else { %>
						<img src="<%- photo %>" class="media" />
					<% }%>	
				</div>
		<% }); %>
	<% } %>
<% } %>
